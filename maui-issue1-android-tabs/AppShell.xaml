<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="maui_issue1_android_tabs.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:maui_issue1_android_tabs"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
       <Tab Title="Home" Icon="house">
           <ShellContent Title="Home 1" ContentTemplate="{DataTemplate views:MainPage}" />
           <ShellContent Title="Home 2" ContentTemplate="{DataTemplate views:MainPage}" />
       </Tab>
       <Tab Title="Other" Icon="information">
           <ShellContent Title="Other 1" ContentTemplate="{DataTemplate views:MainPage}" />
           <ShellContent Title="Other 2" ContentTemplate="{DataTemplate views:MainPage}" />
       </Tab>
    </TabBar>

</Shell>

